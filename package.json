{
  "name": "word-clock",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "ngrok": "ngrok http 4200 --host-header=\"localhost:4200\" --subdomain=designsbysm -bind-tls=true",
    "sw:build": "ng build --watch --c sw",
